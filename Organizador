package CapaNegocio;

public class Organizador extends Persona {
    public String cargo;
    public String horasTrabajo;

    public String getCargo() {
        return cargo;
    }

    public void setCargo(String cargo) {
        this.cargo = cargo;
    }

    public String getHorasTrabajo() {
        return horasTrabajo;
    }

    public void setHorasTrabajo(String horasTrabajo) {
        this.horasTrabajo = horasTrabajo;
    }

    public String getNombres() {
        return nombres;
    }

    public void setNombres(String nombres) {
        this.nombres = nombres;
    }

    public String getApellidos() {
        return apellidos;
    }

    public void setApellidos(String apellidos) {
        this.apellidos = apellidos;
    }

    public String getDni() {
        return dni;
    }

    public void setDni(String dni) {
        this.dni = dni;
    }
     public String Gestionar()
    {
        return "El metodo Gestionar no esta implementado";
    }
    public String Capacitar()
    {
        return "El metodo Dirigir no esta implementado";
    }

}
